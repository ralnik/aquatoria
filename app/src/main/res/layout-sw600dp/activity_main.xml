                        app:left_thumb_image="@drawable/thumb"
                        app:left_thumb_image_pressed="@drawable/thumb"
                        app:max_value="55"
                        app:min_value="2"
                        app:right_thumb_image="@drawable/thumb"
                        app:right_thumb_image_pressed="@drawable/thumb" />

