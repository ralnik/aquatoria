                            android:layout_width="61dp"
                            android:layout_height="79dp"
                            android:layout_marginEnd="27dp"
                            android:onClick="additionalAttriburesButtonsOnClick"
                            app:srcCompat="@drawable/btn_balcon" />

                        <ImageView
                            android:id="@+id/btnKladovaya"
